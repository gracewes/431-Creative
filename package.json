{
  "name": "431-Creative",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "standard && mocha; cucumber-js",
    "client": "parcel client/index.html --port 3000",
    "database": "docker run --name mongo-dev-431 -p 27017:27017 -d mongo & > /dev/null",
    "start": "npm run database; standard --fix; NODE_ENV=development nodemon app.js",
    "prod": "parcel build --experimental-scope-hoisting client/index.html; NODE_ENV=production node app.js",
    "stop": "docker stop mongo-dev-431; docker rm mongo-dev-431",
    "format": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeff-hykin/431-Creative.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeff-hykin/431-Creative/issues"
  },
  "homepage": "https://github.com/jeff-hykin/431-Creative#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "css-baseline": "^1.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "parcel-bundler": "^1.11.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "parcel": "^1.11.0",
    "sinon": "^7.2.3",
    "standard": "^12.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      ".cache/*"
    ]
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "fetch"
    ]
  },
  "settings": {
    "database": {
      "DB_NAME": "creative-db",
      "PROD_DB": "localhost:27017",
      "DEV_DB": "localhost:27017"
    },
    "PORT": 3000
  }
}
